{
  "name": "snapshot-testing",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@lambdatest/selenium-driver": "^1.0.8",
    "@testing-library/dom": "^10.4.1",
    "dotenv": "^17.2.3",
    "prop-types": "^15.8.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-scripts": "5.0.1",
    "selenium-webdriver": "^4.36.0",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:update": "jest --updateSnapshot",
    "test:lambdatest": "node test-runner.js",
    "test:lambdatest:smartui": "npx smartui --config .smartui.json exec -- npm run test:lambdatest",
    "test:smartui": "npx smartui --config .smartui.json exec -- jest src/lambdatest.spec.js --testPathIgnorePatterns=/node_modules/",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@lambdatest/smartui-cli": "^4.1.43",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "babel-jest": "^29.7.0",
    "jest-junit": "^16.0.0"
  }
}
